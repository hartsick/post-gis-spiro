<div id="map-canvas">
</div>

<script>

	// Display map
	var map;

	// Set map styles
	var styles = [
	  {
	    featureType: "all",
	    elementType: "geometry",
	    stylers: [
	      { gamma: 1.6 },
	      { lightness: -5 },
	      { saturation: -100 }
	    ]
	  },
	  {
	    featureType: "road",
	    elementType: "geometry",
	    stylers: [
	      { lightness: 100 },
	      { visibility: "simplified" },
	      { gamma: 10 }
	    ]
	  },  
	  {
	    featureType: "transit",
	    elementType: "geometry",
	    stylers: [
	      { visibility: "simplified" },
	      { gamma: 10 },
	      { lightness: 100 }
	    ]
	  },      
	  {
	    featureType: "water",
	    elementType: "geometry",
	    stylers: [
	      { lightness: -30 }
	    ]
	  },    
	  {
	    featureType: "all",
	    elementType: "labels",
	    stylers: [
	      { visibility: "off" }
	    ]
	  }
	];

	// set up map
	function initialize() {
	  var mapOptions = {
	    zoom: 15,
	    center: {lat: 10, lng: 10},
	    styles: styles
	  };
	  map = new google.maps.Map(document.getElementById('map-canvas'),
	      mapOptions);
	}
	google.maps.event.addDomListener(window, 'load', initialize);

</script>